<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="root" prefHeight="440.0" prefWidth="639.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainPageController">
   <children>
      <Text fill="RED" layoutX="150.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Super Market">
         <font>
            <Font name="Berlin Sans FB Bold" size="65.0" />
         </font>
      </Text>
      <ImageView fx:id="imgCustomer" fitHeight="82.0" fitWidth="84.0" layoutX="170.0" layoutY="156.0" onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation" onMouseExited="#playMouseExitAnimation" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/icons8-customers-64.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgItem" fitHeight="82.0" fitWidth="82.0" layoutX="300.0" layoutY="156.0" onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation" onMouseExited="#playMouseExitAnimation" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/icons8-hide-item-48.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgPlaceOrder" fitHeight="72.0" fitWidth="82.0" layoutX="450.0" layoutY="161.0" onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation" onMouseExited="#playMouseExitAnimation" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/icons8-add-shopping-cart.gif" />
         </image>
      </ImageView>

      <Label fx:id="lblMenu" alignment="CENTER" layoutX="21.0" layoutY="284.0" prefHeight="45.0" prefWidth="577.0" text="  Welcome" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="Berlin Sans FB Bold" size="31.0" />
         </font>
      </Label>
      <Label fx:id="lblDescription" alignment="CENTER" layoutX="37.0" layoutY="341.0" prefHeight="16.0" prefWidth="577.0" text="   Please select one of above main operations to proceed" />
   </children>
</AnchorPane>
